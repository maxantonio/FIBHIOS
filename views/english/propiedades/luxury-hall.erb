<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->

<%= erb :"english/layouts/head" %>

<body>

<%= erb :"english/layouts/preloader" %>

<%= erb :"english/layouts/topbar" %>

<%= erb :"english/layouts/header" %>



<!--wrapper-->
<div class="theme-layout">


  <!--Breadcump-->
  <section class="wrapper-pgtitle">
    <div class="container">
      <div class="calloutbread">

        <h2>  Luxury Hall </h2>

        <ul class="breadcrumb">

          <li><a href="#"> Portfolio </a></li>

          <li>Underlying Properties</li>

        </ul>
      </div>
    </div>


    <div id="triangle-up"><a href="#" class="bonfire-slideout-button">
      <div class="panel-bonfire"><i class="fa fa-plus"></i></div>
    </a></div>
  </section>
  <!--Breadcump END-->


  <!--Contenido -->
  <section id="contenido" class="white-wrapper">
    <div class="container">
      <div class="row">
        <div id="content" class="col-lg-12 col-sm-12 col-md-12 col-xs-12 single-portfolio">


          <!--Parte de Imagenes-->
          <div class="col-sm-4 padding-top">
            <div class="portfolio_item">

              <!--imagen grande-->
              <div class="entry">
                <img src="/images/fibhios/propiedades/LuxuryHall/luxuryhallx380.jpg" alt="" class="img-responsive">
              </div>
                 <!-- /botones de propiedades -->
              <div class="text-center">
              <br>
              <div class="btn-group">
                <a href="/en/propiedades/antara"><button type="button" class="btn btn-primary-bd">Back</button></a>
                <a href="/en/propiedades/oficinas-ci-banco"><button type="button" class="btn btn-primary-bd">Forward</button></a>
              </div>
              </div>
              <!-- /botones de propiedades -->

            </div>
          </div>
          <!--Parte de Imagenes END-->



          <!--Parte de Texto-->
          <div class="col-sm-8 padding-left-40">
            <div class="product_details">

              <!--titulo-->
              <h3>Luxury Hall in numbers</h3>

              <!--tablita-->
              <ul>
                <li><strong>Opening date:</strong>            2010</li>
                <li><strong>LA:</strong>                               233,038 sf</li>
                <li><strong>Privative LA*: </strong>                     115,292 sf</li>
                <li><strong>Property manager: </strong>                GSM</li>
                <li><strong>Visitors per year: </strong>     3.8 million</li>
              </ul>
              <p class="legenda">* Leasable Area of the privative creditor unit of the loan (guarantee).</p>
            </div>

            <!--texto de descripcion-->
            <p>Luxury Hall is a premium shopping mall located in Puebla, with a Leasable Area (LA) of 1.22 msf, and 100% occupancy. Its architecture is distinguished by its iconic ceiling made of salam wood, and its superior dome. It contains 36 stores and a central area with a restaurant-bar. Luxury Hall was developed in record time, opening to the public in 2010.</p>


          </div>
          <!--Parte de Texto-->


          <div class="clearfix"></div>

        </div>
      </div>
      <br>
    </div>
  </section>

  <!--Contenido END -->



  <%= erb :"english/layouts/numeros-destacados" %>

  <%= erb :"english/layouts/prospecto" %>

  <%= erb :"english/layouts/twitter" %>

  <%= erb :"english/layouts/footer" %>

  <%= erb :"english/layouts/copyrights" %>

</div>

<!--CONTENT END-->

<!--Pagina off canvas-->
<%= erb :"english/layouts/off-canvas-page"%>

<!--Flechita de scroll-->
<%= erb :"english/layouts/scroll-to-top" %>


<!--Javascripts-->
<%= erb :"english/layouts/js-contenidos" %>


</body>
</html>
