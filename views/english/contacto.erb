<!--wrapper-->
<div class="theme-layout">


  <!--Breadcump-->
  <section class="wrapper-pgtitle">
    <div class="container">
      <div class="calloutbread">

        <h2>  Contact Us </h2>

      </div>
    </div>


   
  </section>
  <!--Breadcump END-->


  <!--Contenido -->
  <section class="white-wrapper nopadding">
    <div id="map"></div>
    <div class="container">
      <div class="section-title-heading margin-top">
        <h3 class="section-title">LEAVE  A <span class="font-light">MESSAGE</span></h3>
      </div>
      <div class="contact_form">
        <div id="message"></div>
        <form id="contactform" action="/" name="contactform" method="get">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <input type="text" name="name" id="name" class="form-control" placeholder="Name">
            <input type="text" name="email" id="email" class="form-control" placeholder="Email">
            <input type="text" name="website" id="website" class="form-control" placeholder="Website">
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <input type="text" name="subject" id="subject" class="form-control" placeholder="Subject">
            <textarea class="form-control" name="comments" id="comments" rows="6" placeholder="Message"></textarea>
            <button type="submit" value="SEND" id="submit" class="btn btn-lg btn-primary pull-right button-no-relative">Send Message</button>
          </div>
        </form>
        <br>
      </div><!-- end contact-form --><br><br>
    </div><!-- end container -->
  </section>

  <!--Contenido END -->






  

  <%= erb :"/english/layouts/twitter" %>

  <%= erb :"/english/layouts/footer" %>

  <%= erb :"/english/layouts/copyrights" %>

