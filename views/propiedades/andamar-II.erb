              <!--tablita-->
              <ul>
                <li><strong>Año de construcción:</strong>                Febrero 2015</li>
                <li><strong>AR: </strong>                               29,800 m²</li>
                <li><strong>AR Privativo*:</strong>                     29,800 m²</li>
